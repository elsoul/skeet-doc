    "intercept-stdout": "0.1.2",
    "mdast-util-gfm-table": "1.0.7",
    "mdast-util-to-string": "3.1.1",
    "node-fetch": "2.6.7",
    "npm-check-updates": "16.7.10",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.4",
    "raw-loader": "4.0.2",
    "rehype-code-titles": "1.2.0",
